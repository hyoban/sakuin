{
	"name": "sakuin",
	"type": "module",
	"version": "0.2.6",
	"packageManager": "pnpm@8.15.7",
	"description": "SDK for XLog",
	"author": {
		"name": "hyoban",
		"url": "https://github.com/hyoban"
	},
	"license": "MIT",
	"homepage": "https://github.com/hyoban/sakuin#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyoban/sakuin.git"
	},
	"bugs": "https://github.com/hyoban/sakuin/issues",
	"keywords": [],
	"sideEffects": false,
	"exports": {
		".": {
			"import": {
				"types": "./out/index.d.ts",
				"default": "./out/index.js"
			},
			"require": {
				"types": "./out/index.d.cts",
				"default": "./out/index.cjs"
			}
		}
	},
	"main": "./out/index.cjs",
	"module": "./out/index.js",
	"types": "./out/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./out/*",
				"./out/index.d.ts"
			]
		}
	},
	"files": [
		"out"
	],
	"scripts": {
		"build": "next build",
		"build:docs": "typedoc src/lib/storage/index.ts --plugin typedoc-plugin-missing-exports --excludeExternals",
		"build:lib": "tsup src/lib/storage/index.ts --dts --format esm,cjs -d out",
		"codegen": "graphql-codegen",
		"dev": "next dev",
		"dev:docs": "pnpm run build:docs --watch",
		"knip": "knip",
		"lint": "prettier --list-different . && eslint",
		"lint:fix": "prettier --list-different --write . && eslint --fix",
		"prepublishOnly": "pnpm run build:lib",
		"start": "next start",
		"start:docs": "live-server docs",
		"test": "vitest",
		"typecheck": "tsc"
	},
	"dependencies": {
		"@urql/core": "^5.0.0",
		"crossbell": "^1.11.9",
		"nanoid": "^5.0.7"
	},
	"devDependencies": {
		"@egoist/tailwindcss-icons": "^1.7.4",
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.2.5",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@iconify-json/lucide": "^1.1.181",
		"@iconify-json/simple-icons": "^1.1.99",
		"@parcel/watcher": "^2.4.1",
		"@radix-ui/react-label": "^2.0.2",
		"@radix-ui/react-select": "^2.0.0",
		"@radix-ui/react-slot": "^1.0.2",
		"@shikijs/rehype": "^1.3.0",
		"@t3-oss/env-nextjs": "^0.9.2",
		"@tailwindcss/typography": "^0.5.12",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/hast": "^3.0.4",
		"@types/node": "^20.12.7",
		"@types/react": "^18.2.78",
		"@types/react-dom": "^18.2.25",
		"autoprefixer": "^10.4.19",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"eslint": "^9.0.0",
		"eslint-config-hyoban": "^1.0.1",
		"graphql": "^16.8.1",
		"hast-util-from-html": "^2.0.1",
		"image-size": "^1.1.1",
		"jiti": "^1.21.0",
		"jotai": "^2.8.0",
		"jotai-dark": "^0.3.0",
		"jotai-effect": "^1.0.0",
		"knip": "^5.9.4",
		"live-server": "^1.2.2",
		"lucide-react": "^0.368.0",
		"next": "^14.2.1",
		"next-intl": "^3.11.1",
		"next-mdx-remote": "0.0.0-canary-20240314011020",
		"path-to-regexp": "^6.2.2",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-config-hyoban": "^1.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-tweet": "^3.2.1",
		"react-wrap-balancer": "^1.1.0",
		"rehype-raw": "^7.0.0",
		"release-it": "^17.2.0",
		"release-it-pnpm": "^4.1.9",
		"remark-gfm": "^4.0.0",
		"remark-github-alerts": "^0.1.0-beta.3",
		"remark-parse": "^11.0.0",
		"tailwind-merge": "^2.2.2",
		"tailwindcss": "^3.4.3",
		"tailwindcss-animate": "^1.0.7",
		"tsup": "^8.0.2",
		"typedoc": "^0.25.13",
		"typedoc-plugin-missing-exports": "^2.2.0",
		"typescript": "^5.4.5",
		"unified": "^11.0.4",
		"unist-util-visit": "^5.0.0",
		"vitest": "^1.5.0",
		"zod": "^3.22.4"
	},
	"prettier": "prettier-config-hyoban"
}
