{
  "name": "sakuin",
  "type": "module",
  "version": "0.1.0-alpha.4",
  "packageManager": "pnpm@8.15.4",
  "description": "SDK for XLog",
  "author": {
    "name": "hyoban",
    "url": "https://github.com/hyoban"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/sakuin#readme",
  "repository": "hyoban/sakuin",
  "bugs": "https://github.com/hyoban/sakuin/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./out/index.d.ts",
        "default": "./out/index.js"
      },
      "require": {
        "types": "./out/index.d.cts",
        "default": "./out/index.cjs"
      }
    }
  },
  "main": "./out/index.cjs",
  "module": "./out/index.js",
  "types": "./out/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./out/*",
        "./out/index.d.ts"
      ]
    }
  },
  "files": [
    "out"
  ],
  "scripts": {
    "build": "waku build --with-ssr --with-vercel-static",
    "build:lib": "tsup src/lib/storage/index.ts --dts --format esm,cjs -d out",
    "codegen": "graphql-codegen",
    "dev": "waku dev --with-ssr",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build:lib",
    "start": "waku start --with-ssr",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@urql/core": "^4.3.0",
    "crossbell": "^1.11.9"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.7.4",
    "@eslint-react/eslint-plugin": "^1.5.6",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@iconify-json/lucide": "^1.1.173",
    "@iconify-json/simple-icons": "^1.1.95",
    "@parcel/watcher": "^2.4.1",
    "@shikijs/rehype": "^1.1.7",
    "@t3-oss/env-core": "^0.9.2",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-hyoban": "^0.1.39",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.8.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "react": "18.3.0-canary-6c3b8dbfe-20240226",
    "react-dom": "18.3.0-canary-6c3b8dbfe-20240226",
    "react-server-dom-webpack": "18.3.0-canary-6c3b8dbfe-20240226",
    "rehype-stringify": "^10.0.0",
    "release-it": "^17.1.1",
    "release-it-pnpm": "4.0.0-beta.2",
    "remark-github-alerts": "^0.0.4",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "simple-git-hooks": "^2.10.0",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "unified": "^11.0.4",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1",
    "waku": "0.19.4",
    "zod": "^3.22.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
