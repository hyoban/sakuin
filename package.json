{
  "name": "sakuin",
  "type": "module",
  "version": "0.1.0-alpha.12",
  "packageManager": "pnpm@8.15.5",
  "description": "SDK for XLog",
  "author": {
    "name": "hyoban",
    "url": "https://github.com/hyoban"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/sakuin#readme",
  "repository": "hyoban/sakuin",
  "bugs": "https://github.com/hyoban/sakuin/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./out/index.d.ts",
        "default": "./out/index.js"
      },
      "require": {
        "types": "./out/index.d.cts",
        "default": "./out/index.cjs"
      }
    }
  },
  "main": "./out/index.cjs",
  "module": "./out/index.js",
  "types": "./out/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./out/*",
        "./out/index.d.ts"
      ]
    }
  },
  "files": [
    "out"
  ],
  "scripts": {
    "build": "next build",
    "build:lib": "tsup src/lib/storage/index.ts --dts --format esm,cjs -d out",
    "codegen": "graphql-codegen",
    "dev": "next dev",
    "knip": "knip",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build:lib",
    "start": "next start",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@urql/core": "^4.3.0",
    "crossbell": "^1.11.9",
    "nanoid": "^5.0.6"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.7.4",
    "@eslint-react/eslint-plugin": "^1.5.6",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@iconify-json/lucide": "^1.1.174",
    "@iconify-json/simple-icons": "^1.1.95",
    "@next/eslint-plugin-next": "^14.1.3",
    "@parcel/watcher": "^2.4.1",
    "@shikijs/rehype": "^1.2.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/hast": "^3.0.4",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.18",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-hyoban": "^0.2.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.8.1",
    "hast-util-from-html": "^2.0.1",
    "image-size": "^1.1.1",
    "jiti": "^1.21.0",
    "jotai": "^2.7.1",
    "jotai-dark": "^0.3.0",
    "jotai-effect": "^0.6.0",
    "knip": "^5.1.2",
    "lint-staged": "^15.2.2",
    "next": "^14.1.3",
    "next-mdx-remote": "0.0.0-canary-20240314011020",
    "path-to-regexp": "^6.2.1",
    "postcss": "^8.4.36",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tweet": "^3.2.0",
    "react-wrap-balancer": "^1.1.0",
    "release-it": "^17.1.1",
    "release-it-pnpm": "^4.1.1",
    "remark-gfm": "^4.0.0",
    "remark-github-alerts": "^0.1.0-beta.3",
    "remark-parse": "^11.0.0",
    "simple-git-hooks": "^2.11.0",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "vitest": "^1.4.0",
    "zod": "^3.22.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
